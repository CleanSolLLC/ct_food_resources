<div class="text-3xl px-6 py-4">
  <%= "Cities & Towns In #{@county.name} county"%>
</div>
<div class="text-xl px-6 py-4">
  <%= "Total Food Pantry Count For #{@county.name} county: #{@total_by_county}"%>
</div >
<div>
  <ul>
    <% @towns_hash.each_pair do |town, count| %>
      <li class="px-6 py-4">
        <%= link_to "#{town} (#{count})", town %>
      </li>
    <% end %>
  </ul>
</div>







